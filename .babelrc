{
  "presets": [
    "env",
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    ["transform-runtime",  {
	    "helpers": false,
	    "polyfill": false,
	    "regenerator": true,
	    "moduleName": "babel-runtime"
	  }],
    "transform-class-properties",
    "transform-decorators",
    "transform-react-constant-elements",
    "transform-react-inline-elements"
  ]
}
